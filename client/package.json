{
  "name": "memberdb-tool-client",
  "version": "0.1.0",
  "homepage": "https://pineapple.lol/",
  "license": "MIT",
  "dependencies": {
    "ag-grid-community": "^25.0.0",
    "ag-grid-enterprise": "^25.0.0",
    "ag-grid-react": "^25.0.0",
    "axios": "^0.21.1",
    "draft-js": "^0.11.7",
    "lodash": "^4.17.20",
    "moment": "^2.29.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-datepicker": "^3.4.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-select": "^3.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "cypress": "^6.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "flow-bin": "^0.108.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint src --ext js,jsx",
    "lint:fix": "eslint --fix src --ext js,jsx",
    "format": "prettier --write \"./**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,json,md}\"",
    "flow": "flow",
    "build": "react-scripts build",
    "test": "yarn run cypress run",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,css,scss,json,md,jsx}": [
      "prettier --write"
    ],
    "./client/**/*.{js,jsx}": [
      "eslint --fix -c client/.eslintrc.json"
    ]
  }
}
